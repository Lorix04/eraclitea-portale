{
  "name": "portale-formazione",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:prod": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:reset": "prisma migrate reset --force",
    "seed": "tsx prisma/seed.ts",
    "seed:fresh": "npm run prisma:reset && npm run seed",
    "docker:dev": "docker compose -f docker-compose.dev.yml up -d",
    "docker:dev:down": "docker compose -f docker-compose.dev.yml down",
    "docker:dev:logs": "docker compose -f docker-compose.dev.yml logs -f",
    "docker:prod": "docker compose up -d --build",
    "docker:prod:down": "docker compose down",
    "setup": "npm run docker:dev && node -e \"setTimeout(()=>{},5000)\" && npm run prisma:push && npm run seed",
    "setup:fresh": "npm run docker:dev:down && npm run docker:dev && node -e \"setTimeout(()=>{},5000)\" && npm run prisma:reset && npm run seed",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "check": "npm run lint && npm run type-check",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ag-grid-community/core": "^32.3.3",
    "@ag-grid-community/react": "^32.3.3",
    "@ag-grid-community/styles": "^32.3.3",
    "@auth/prisma-adapter": "^1.6.0",
    "@aws-sdk/client-s3": "^3.645.0",
    "@aws-sdk/s3-request-presigner": "^3.645.0",
    "@hookform/resolvers": "^3.3.4",
    "@prisma/client": "^5.14.0",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-select": "^2.2.6",
    "@tanstack/react-query": "^5.32.0",
    "@tanstack/react-table": "^8.11.8",
    "@vercel/analytics": "^1.3.1",
    "@vercel/speed-insights": "^1.0.12",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "csv-parse": "^5.5.6",
    "csv-stringify": "^6.5.1",
    "date-fns": "^3.6.0",
    "lucide-react": "^0.468.0",
    "next": "14.1.0",
    "next-auth": "^5.0.0-beta.20",
    "nodemailer": "^7.0.12",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.51.0",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.44.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "@types/react": "18.2.66",
    "@types/react-dom": "18.2.22",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.1.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "node-mocks-http": "^1.13.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prisma": "^5.14.0",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.4.5"
  }
}
